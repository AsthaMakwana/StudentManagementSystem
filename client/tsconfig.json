{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "module": "esnext",
    "allowJs": true,
    "types": ["react", "react-dom", "node"]
  },
  "include": ["src"]
}
